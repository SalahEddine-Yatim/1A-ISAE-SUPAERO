{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug app-linked-list-int",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/app-linked-list-int",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build app-linked-list-int",
        }
    ]
}
