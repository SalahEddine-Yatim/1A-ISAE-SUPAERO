#include <stdio.h>

int main(void){

    float my_array[10];
    float min = my_array[0];
    int indx ;
    for (int i=1 ; i<10 ; i++){
        if (my_array[i] <= min){;
        min = my_array[i];
        indx = i ;
    }
    }
    

    printf("The minimum is %f and is located at index %d", min, indx);

    return 0;
}